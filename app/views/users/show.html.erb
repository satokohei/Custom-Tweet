<div style="width: 300px; margin: auto;"class="">
  <h2><%= @user.name %>のおすすめ</h2>
   
  <ul class="">
  <%@tweets.each do |tweet|%>
    <li style="background-image: url(<%= tweet.image %>); background-repeat: no-repeat; width: 300px;
    height: 300px; list-style:none">
    </li>
    <li>ドリンク名
    <%= tweet.drink_name %></li>
    <li>追加したカスタム
    <%= tweet.custom_name %></li>
  <% end %>
  </ul>

  <%= link_to '前のページに戻る', "/tweets
  ", method: :get %>
</div>
